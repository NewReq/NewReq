<p id="notice"><%= notice %></p>
<br>
<div class="container">
  <h4>Listing Public Domains</h4>
  <p></p> 
  <br>
  <br>
  <%= link_to 'New Publicdomain', new_publicdomain_path, class: "btn btn-lg btn-primary" %>
  <br>
  <br>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Domain</th>
        <th colspan="3">Actions</th>
      </tr>
    </thead>

    <tbody>
      <% @publicdomains.each do |publicdomain| %>
        <tr>
          <td><%= publicdomain.domain %></td>
          <td><%= link_to 'Show', publicdomain, class: "btn btn-primary" %></td>
          <td><%= link_to 'Edit', edit_publicdomain_path(publicdomain), class: "btn btn-warning" %></td>
          <td><%= link_to 'Destroy', publicdomain, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

<br>


