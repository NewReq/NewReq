<p id="notice"><%= notice %></p>

<h1>New Users</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Username</th>
      <th>Title</th>
      <th>Company</th>
      <th>Site</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @newusers.each do |newuser| %>
      <tr>
        <td><%= newuser.id %></td>
        <td><%= newuser.firstname %></td>
        <td><%= newuser.lastname %></td>
        <td><%= newuser.username %></td>
        <td><%= newuser.title %></td>
        <td><%= newuser.company_id %></td>
        <td><%= newuser.site_id %></td>
        <td><%= link_to 'Add to LDAP', newuser %></td>
        <td><%= link_to 'Ignore', edit_newuser_path(newuser) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

